# --- Database ---
DB_NAME=trapsensor
DB_USER=trapsensor_user
DB_PASSWORD=your_secure_password
DB_ROOT_PASSWORD=your_root_password
DB_HOST=db
DB_PORT=3306

# --- JWT ---
JWT_SECRET=generate_a_long_random_string_here

# --- Pushover (Optional) ---
PUSHOVER_API_TOKEN=your_app_token

# --- VAPID (Web-Push) ---
VITE_VAPID_PUBLIC_KEY=your_public_key
VAPID_PRIVATE_KEY=your_private_key
VAPID_EMAIL=admin@example.com

# --- MQTT (Internal Broker is on 1884) ---
# For LoRaWAN (TTN), these are usually configured in the TTN Console
NBIOT_MQTT_BROKER=trapsensor_app
NBIOT_MQTT_PORT=1884
